{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "off",
    "no-restricted-syntax": "off",
    "no-prototype-builtins": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "no-await-in-loop": "off",
    "no-plusplus": "off"
  }
}